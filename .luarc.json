{
	"runtime.version": "LuaJIT",
	"runtime.path": [
		"?.lua",
		"?/init.lua",
		"3rd-deps/lua/?.lua",
		"aqua/?.lua",
		"ncdk/?.lua",
		"chartbase/?.lua",
		"libchart/?.lua",
		"3rd-deps/lua/?/init.lua",
		"aqua/?/init.lua",
		"ncdk/?/init.lua",
		"chartbase/?/init.lua",
		"libchart/?/init.lua"
	],
	"runtime.pathStrict": true,
	"runtime.builtin": {
		"utf8": "enable"
	},
	"workspace.ignoreSubmodules": false,
	"workspace.maxPreload": 10000,
	"workspace.ignoreDir": [
		".vscode",
		"*/bench",
		"*/spec",
		"*/test",
		"*/tests",
		"userdata/skins/ignore"
	],
	"workspace.library": [
		"${3rd}/love2d/library",
		"${3rd}/OpenResty/library",
		"~/.config/Code/User/globalStorage/sumneko.lua/addonManager/addons/luasocket/module/library"
	],
	"workspace.checkThirdParty": false,
	"completion.autoRequire": false,
	"diagnostics.disable": [],
	"diagnostics.globals": [
		"love",
		"newproxy"
	],
	"diagnostics.neededFileStatus": {
		"no-unknown": "Any",
		"codestyle-check": "Any"
	}
}
